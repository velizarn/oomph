        excludedTriggers="STARTUP MANUAL"
        option="-Duser.language"
        value="=en"
        vm="true"/>
    <setupTask
        xsi:type="setup:EclipseIniTask"
        excludedTriggers="STARTUP MANUAL"
        option="-Dfile.encoding"
